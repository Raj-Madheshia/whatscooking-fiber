{
    "expo": {
        "name": "FoodTrail",
        "version": "1.1.1",
        "description": "App for sharing photos and recipes for delicious cuisines",
        "sdkVersion": "27.0.0",
        "orientation": "portrait",
        "privacy": "public",
        "icon": "./app.png",
        "primaryColor": "#f27842",
        "splash": {
            "image": "./splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "packagerOpts": {
            "assetExts": ["otf"]
        },
        "androidStatusBar": {
            "backgroundColor": "#000000",
            "barStyle": "dark-content"
        },
        "ios": {
            "bundleIdentifier": "com.wcandillon.fiber"
        }
    }
}
